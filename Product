# MACI-Commerce

package model;

public class Product {
  public String id, name;
  public double price;
  public int stock;

  public Product(String id, String name, double price, int stock) {
    this.id = id;
    this.name = name;
    this.price = price;
    this.stock = stock;
    }

public boolean isAvailable() {
  return stock > 0;
}

//Override
public String toString(){
retrun id + ": " + name _ " ($" + price + ") - Stock: " + stock;
  }
}
